{
  "name": "ngx-lite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run lint && npm run prettier && ng build --prod && ng test @ngx-lite/all --watch=false && npm run e2e && npm run package",
    "test": "ng test @ngx-lite/all",
    "test:watch": "ng test @ngx-lite/all --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod && firebase deploy",
    "coverage": "ng test --watch=false --code-coverage",
    "prettier": "run-s prettier:*",
    "prettier:scss": "prettier --parser=scss --single-quote --write \"**/*.scss\"",
    "prettier:typescript": "prettier --parser=typescript --single-quote --write \"**/*.ts\"",
    "package": "run-s package:*",
    "package:carbon-ad": "ng build @ngx-lite/carbon-ad",
    "package:code-example": "ng build @ngx-lite/code-example",
    "package:debounce-click": "ng build @ngx-lite/debounce-click",
    "package:eq": "ng build @ngx-lite/eq",
    "package:in-viewport": "ng build @ngx-lite/in-viewport",
    "package:input-datepicker": "ng build @ngx-lite/input-datepicker",
    "package:input-timepicker": "ng build @ngx-lite/input-timepicker",
    "package:input-range": "ng build @ngx-lite/input-range",
    "package:input-star-rating": "ng build @ngx-lite/input-star-rating",
    "package:input-switch": "ng build @ngx-lite/input-switch",
    "package:input-tag": "ng build @ngx-lite/input-tag",
    "package:json-ld": "ng build @ngx-lite/json-ld",
    "package:loaders": "ng build @ngx-lite/loaders",
    "package:modal": "ng build @ngx-lite/modal",
    "package:nav-drawer": "ng build @ngx-lite/nav-drawer",
    "package:progress": "ng build @ngx-lite/progress",
    "package:tabs": "ng build @ngx-lite/tabs",
    "package:menus": "ng build @ngx-lite/menus"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "blueprint-css": "^3.0.0-beta.5",
    "core-js": "^2.5.4",
    "date-fns": "^1.29.0",
    "normalize.css": "^8.0.0",
    "prismjs": "^1.14.0",
    "resize-observer-polyfill": "^1.5.0",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "~6.1.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "1.12.1",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
