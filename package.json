{
  "name": "ngx-lite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run lint && npm run prettier && npm run package && ng test @ngx-lite/all --watch=false && ng build --prod",
    "test": "ng test @ngx-lite/all --watch=false",
    "test:watch": "ng test @ngx-lite/all --watch --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod && firebase deploy",
    "coverage": "cd coverage && lite-server",
    "prettier": "run-s prettier:*",
    "prettier:scss": "prettier --parser=scss --single-quote --write \"**/*.scss\"",
    "prettier:typescript": "prettier --parser=typescript --single-quote --write \"**/*.ts\"",
    "package": "run-s package:*",
    "package:util": "ng build @ngx-lite/util",
    "package:carbon-ad": "ng build @ngx-lite/carbon-ad",
    "package:code-example": "ng build @ngx-lite/code-example",
    "package:debounce-click": "ng build @ngx-lite/debounce-click",
    "package:eq": "ng build @ngx-lite/eq",
    "package:in-viewport": "ng build @ngx-lite/in-viewport",
    "package:input-datepicker": "ng build @ngx-lite/input-datepicker",
    "package:input-timepicker": "ng build @ngx-lite/input-timepicker",
    "package:input-range": "ng build @ngx-lite/input-range",
    "package:input-star-rating": "ng build @ngx-lite/input-star-rating",
    "package:input-switch": "ng build @ngx-lite/input-switch",
    "package:input-tag": "ng build @ngx-lite/input-tag",
    "package:json-ld": "ng build @ngx-lite/json-ld",
    "package:loaders": "ng build @ngx-lite/loaders",
    "package:modal": "ng build @ngx-lite/modal",
    "package:nav-drawer": "ng build @ngx-lite/nav-drawer",
    "package:progress": "ng build @ngx-lite/progress",
    "package:tabs": "ng build @ngx-lite/tabs",
    "package:menus": "ng build @ngx-lite/menus"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.1",
    "@angular/common": "~11.0.1",
    "@angular/compiler": "~11.0.1",
    "@angular/core": "~11.0.1",
    "@angular/forms": "~11.0.1",
    "@angular/platform-browser": "~11.0.1",
    "@angular/platform-browser-dynamic": "~11.0.1",
    "@angular/router": "~11.0.1",
    "@types/prismjs": "^1.16.2",
    "blueprint-css": "^3.1.1",
    "date-fns": "^2.16.1",
    "normalize.css": "^8.0.0",
    "prismjs": "^1.23.0",
    "resize-observer-polyfill": "^1.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "2.2.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
